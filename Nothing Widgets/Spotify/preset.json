                    "shape_type": "RECT",
                    "fx_mask": "CLIP_ALL",
                    "internal_toggles": {
                      "shape_width": 10,
                      "shape_height": 10
                    },
                    "internal_formulas": {
                      "shape_width": "$if(si(rwidth)<si(rheight)*0.6, si(rwidth)-36/si(skpi), si(rheight)*0.6-36/si(skpi))$",
                      "shape_height": "$si(rheight)*0.6-36/si(skpi)$"
                    }
                  },
                  {
                    "internal_type": "OverlapLayerModule",
                    "internal_title": "Bg",
                    "viewgroup_items": [
                      {
                        "internal_type": "ShapeModule",
                        "shape_type": "RECT",
                        "internal_toggles": {
                          "shape_width": 10,
                          "shape_height": 10,
                          "paint_color": 10
                        },
                        "internal_formulas": {
                          "shape_width": "$if(si(rwidth)<si(rheight)*0.6, si(rwidth)-36/si(skpi), si(rheight)*0.6-36/si(skpi))$",
