                    "internal_type": "BitmapModule",
                    "bitmap_scale_mode": "CENTER_CROP",
                    "internal_toggles": {
                      "bitmap_bitmap": 10,
                      "bitmap_width": 10,
                      "bitmap_height": 10,
                      "bitmap_alpha": 10
